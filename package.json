{
  "name": "stapp",
  "version": "0.0.1",
  "description": "A Frontend boilerplate",
  "homepage": "https://github.com/theaidem/stapp",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack": "NODE_ENV=production ./node_modules/.bin/webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "deploy:gh-pages": "GH_PAGES=true npm run build && ./node_modules/.bin/gh-pages -d dist && npm run clean",
    "start": "./node_modules/.bin/webpack-dev-server --config webpack.config.dev.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Max Kokorin",
  "license": "MIT",
  "dependencies": {
    "history": "^1.17.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^4.1.2",
    "react-router": "^1.0.3",
    "react-router-redux": "^2.0.4",
    "redux": "^3.1.4",
    "redux-logger": "^2.4.0",
    "redux-thunk": "^1.0.3",
    "semantic-ui-container": "^2.1.7",
    "semantic-ui-header": "^2.1.7",
    "semantic-ui-icon": "^2.1.7",
    "semantic-ui-loader": "^2.1.7",
    "semantic-ui-menu": "^2.1.7",
    "semantic-ui-reset": "^2.1.7",
    "semantic-ui-site": "^2.1.7"
  },
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "gh-pages": "^0.9.0",
    "eventsource-polyfill": "^0.9.6",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^1.7.0",
    "react-transform-hmr": "^1.0.1",
    "rimraf": "^2.5.1",
    "style-loader": "^0.13.0",
    "webpack-dev-server": "^1.14.1",
    "webpack": "^1.12.12",
    "webpack-hot-middleware": "^2.6.4"
  }
}
